<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<!-- 当前项目在分布式系统中的唯一名称，计算依赖管理的标签  -->
	<dubbo:application  name="userservice" />
	
	<!-- 这个服务要暴露的服务地址  -->
	<dubbo:registry  address="N/A"/>
	
	<dubbo:reference  id="orderservice"  interface="com.gupao.edu.service.IOrderService"  
			url="dubbo://192.168.203.1:20880/com.gupao.edu.service.IOrderService" />
	
</beans>